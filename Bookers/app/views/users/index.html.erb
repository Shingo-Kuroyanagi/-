<%= render 'books/list', book: @book, user: current_user%>
<h2>Users</h2>

<div class="container">
     <div class="col-xs-9">
     <table class="table table-hover">
  <thead>
   <tr>
    <th></th>
    <th>Image</th>
    <th>name</th>
   </tr>
  </thead>

  <tbody>
     <% @users.each do |user|%>
    <tr>
     <td>
  　  <%= attachment_image_tag user,:profile_image ,format:'jpeg',fallback: "no_image.jpg", size: '60x60'%>
     </td>
     <td>
  　   <%=user.name %>
     </td>
     <td>
     <%= link_to 'Show',user_path(user.id)%>
　　 </td>
    </tr>
  <% end %>
    </tbody>
  </table>
 </div>
</div>